{
  "name": "salla-asnmt-app",
  "version": "0.0.1",
  "description": "Reusable web components for Salla mini checkout using Stencil and TailwindCSS",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/salla-asnmt-app/salla-asnmt-app.esm.js",
  "exports": {
    ".": {
      "import": "./dist/salla-asnmt-app/salla-asnmt-app.esm.js",
      "require": "./dist/salla-asnmt-app/salla-asnmt-app.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "typescript": "^5.8.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0",
    "tailwindcss": "^4.1.5",
    "stencil-tailwind-plugin": "^2.0.5"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  }
}
